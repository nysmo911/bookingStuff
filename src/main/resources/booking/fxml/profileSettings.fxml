                <Label fx:id="lastNameLabel" layoutX="120.0" layoutY="77.0" style="-fx-font-family: 'Impact'; -fx-font-size: 13; -fx-text-fill: #333;" text="LName" />
                <Label fx:id="emailLabel" layoutX="120.0" layoutY="107.0" style="-fx-font-family: 'Impact'; -fx-font-size: 13; -fx-text-fill: #333;" text="Email" />
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
